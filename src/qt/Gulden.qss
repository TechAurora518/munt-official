GUI QScrollBar:vertical
{
    margin: 0px;
    border: 0;
    background-color: transparent;
    width: 16px;
    border-radius: 4px;
    padding-top: 15px;
    padding-bottom: 15px;
    border-right: 8px solid rgba(0, 0, 0, 0%);
}

GUI QScrollBar::handle:vertical
{
    background-color: #ccc;
    min-height: 10px;
    max-height: 10px;
    max-width: 8px;
    border-radius: 4px;
}

GUI QScrollBar::handle:vertical:hover
{
    background-color: TEXT_COLOR_1;
}

GUI QScrollBar::handle:vertical:pressed
{
    background-color: #888;
}

GUI QScrollBar::add-line
{
    background-color: transparent;
}

GUI QScrollBar::sub-line
{
    background-color: transparent;
}

GUI QScrollBar::add-page
{
    background-color: transparent;
}

GUI QScrollBar::sub-page
{
    background-color: transparent;
}


GUI QScrollBar:down-arrow:vertical
{
    width: 8px;
    height: 8px;
    border-top: 16px solid rgba(0, 0, 0, 0%);
    border-right: 8px solid rgba(0, 0, 0, 0%);
    image: url(':/Gulden/angle_down_grey');
}

GUI QScrollBar:down-arrow:vertical:hover
{
    image: url(':/Gulden/angle_down_blue');
    border-right: 8px solid rgba(0, 0, 0, 0%);
}

